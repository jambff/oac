{
  "include": [
    "src/*",
    "src/__mocks__/*",
  ],
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es5",
    "declaration": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declarationMap": true,
    "resolveJsonModule": true
  }
}
